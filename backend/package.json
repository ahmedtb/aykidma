{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "commit": "node -e \"const mssg = process.argv[1]; require('child_process').execSync('npm run prod && git add . && git commit -m \\\"' + mssg + '\\\" && git push', { stdio:[0, 1, 2] })\""
    },
    "devDependencies": {
        "@babel/preset-react": "^7.13.13",
        "axios": "^0.21.4",
        "bootstrap": "^5.1.3",
        "jquery": "^3.6",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.19",
        "moment": "^2.29.4",
        "popper.js": "^1.16.1",
        "postcss": "^8.3.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1",
        "react-router-bootstrap": "^0.25.0",
        "resolve-url-loader": "^3.1.2",
        "rtl-bootstrap": "0.0.5",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.4"
    },
